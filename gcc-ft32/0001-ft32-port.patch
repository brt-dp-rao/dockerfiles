+        ldi.l   $r6,$r0,4
+        ldi.l   $r7,$r0,8
+        ldi.l   $r8,$r0,12
+        ldi.l   $r9,$r0,16
+        ldi.l   $r10,$r0,20
+        ldi.l   $r11,$r0,24
+        ldi.l   $r12,$r0,28
