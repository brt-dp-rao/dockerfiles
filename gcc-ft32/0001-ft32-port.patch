+setjmp:
+        pop.l   $r5           # return address in $r5
+        sti.l   $r0,0,$r5
+        sti.l   $r0,4,$r6
+        sti.l   $r0,8,$r7
+        sti.l   $r0,12,$r8
+        sti.l   $r0,16,$r9
