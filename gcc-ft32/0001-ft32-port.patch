+        sti.l   $r0,84,$r26
+        sti.l   $r0,88,$r27
+        sti.l   $r0,92,$r28
+        sti.l   $r0,96,$r29
+        sti.l   $r0,100,$r30
+        sti.l   $r0,104,$r31
